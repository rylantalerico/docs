    "url": "https://app.crate.fm"
  },
  "tabs": [],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://support.crate.fm/"
    },
    {
      "name": "Contact us",
      "icon": "bell-concierge",
      "url": "mailto:support@crate.fm"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://crate.fm/blog"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
