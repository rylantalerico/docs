{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Crate.fm",
  "logo": {
    "dark": "/logo/cratefm-logo-h96px-dark.png",
    "light": "/logo/cratefm-logo-h96px-light.png"
  },
  "favicon": "/favicon-updated.ico",
  "colors": {
    "primary": "#5260FF",
    "light": "#7580FF",
    "dark": "#424DCC",
    "anchors": {
      "from": "#7580FF",
      "to": "#5260FF"
    }
  },
  "topbarLinks": [
